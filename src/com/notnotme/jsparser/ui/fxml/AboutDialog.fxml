<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox fx:id="mRoot" alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.notnotme.jsparser.controller.ui.AboutDialogController">
	<children>
		<ImageView pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@../../res/iimlogo.png" />
			</image>
		</ImageView>
		<HBox alignment="CENTER" spacing="5.0">
			<children>
				<Label text="%appname">
					<font>
						<Font name="System Bold" size="13.0" />
					</font>
				</Label>
				<Label text="%appversion">
					<font>
						<Font name="System Bold" size="13.0" />
					</font>
				</Label>
			</children>
			<padding>
				<Insets left="10.0" right="10.0" top="15.0" />
			</padding>
		</HBox>
		<HBox alignment="CENTER">
			<children>
				<Label text="%use" />
				<Hyperlink fx:id="mMuxmLink" text="muxm" />
			</children>
			<VBox.margin>
				<Insets bottom="15.0" />
			</VBox.margin>
		</HBox>
		<HBox alignment="CENTER">
			<children>
				<Label text="%soundby" />
				<Hyperlink fx:id="mSongLink" text="Ultrasyd" />
			</children>
			<VBox.margin>
				<Insets />
			</VBox.margin>
		</HBox>
		<HBox alignment="CENTER">
			<children>
				<Label text="%iconsby" />
				<Hyperlink fx:id="mIconsLink" text="damieng" />
			</children>
			<padding>
				<Insets bottom="15.0" />
			</padding>
		</HBox>
		<Hyperlink fx:id="mWebsiteLink" alignment="CENTER" text="http://www.notnotme.com" />
		<Hyperlink fx:id="mGithubLink" text="http://www.github.com/notnotme" />
	</children>
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
</VBox>
